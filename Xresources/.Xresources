xterm*faceName: Iosevka
xterm*faceSize: 12
xterm*scrollBar: false
xterm*scrollTtyOutput: false
xterm*vt100.translations: #override \n\
 Shift <Key>Home: scroll-forw(10000000000) \n\
 Shift <Key>End: scroll-back(10000000000)

! special
*.foreground:   #f3feff
*.background:   #1b1e25
*.cursorColor:  #f3feff

! black
*.color0:       #1b1e25
*.color8:       #2f3542

! red
*.color1:       #C7726D
*.color9:       #ff8279

! green
*.color2:       #BF807F
*.color10:      #ff9290

! yellow
*.color3:       #CF8670
*.color11:      #ffa37c

! blue
*.color4:       #5B7091
*.color12:      #668ecb

! magenta
*.color5:       #9E758D
*.color13:      #df86ba

! cyan
*.color6:       #6F98B9
*.color14:      #7dc7ff

! white
*.color7:       #c4cdd7
*.color15:      #f3feff

