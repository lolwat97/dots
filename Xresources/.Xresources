! red
*.color1:       #6D91A1
*.color9:       #7cc3e2

! green
*.color2:       #536D8D
*.color10:      #5d8cc5

! yellow
*.color3:       #96A5A5
*.color11:      #aeeaea

! blue
*.color4:       #3C5483
*.color12:      #4169b6

! magenta
*.color5:       #A9BBBE
*.color13:      #c4ffff

! cyan
*.color6:       #7AB5C3
*.color14:      #8af7ff

! white
*.color7:       #cbe1e4
*.color15:      #f0ffff

